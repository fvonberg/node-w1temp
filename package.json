  "dependencies": {
    "gpio": "^0.2.7"
  },
  "devDependencies": {
    "@types/node": "^9.6.1",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.13.2",
    "cpx": "^1.5.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^5.0.1",
    "eslint-plugin-import": "^1.12.0"
